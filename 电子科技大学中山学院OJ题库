Problem1542:
#include<stdio.h>

int main()

{

	int T, n, m;

	long long res=1;

	scanf("%d", &T);

	if (T >= 1 && T <= 100000)

	{

		for (int i = 0; i < T; i++)

		{

			scanf("%d", &n);

			if (n >= 1 && n <= 4)

			{

				for (int j = 0; j < n; j++)

				{

					scanf("%d", &m);

					res = res *m;

				}

			}

			if (res >100000000)

			{

				printf("TLE\n");

				res = 1;

			}

			else

			{

				printf("AC\n");

				res = 1;

			}

		}

	}

	return 0;

}
