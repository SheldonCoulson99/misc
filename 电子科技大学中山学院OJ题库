Problem1101:
#include <stdio.h>

int main()

{

	int n, i, j, a = 1, b = 1, k;

	scanf("%d", &n);

	for (i = 1; i <= n; i++)

	{

		b = a;

		a = a + i;

		k = i + 1;

		for (j = i; j <= n; j++)

		{

			if (j == n)

			{

				printf("%d\n", b);

			}

			else

			{

				printf("%d ", b);

			}

			b += k;

			k++;

		}

	}

	return 0;

}
