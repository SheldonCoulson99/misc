Problem1023:
#include <stdio.h>

int main()

{

	char a, b, c, op;

	scanf("%c%c%c", &a, &b, &c);

	{

		if (a > b) op = a, a = b, b = op;

		if (a > c) op = a, a = c, c = op;

		if (b > c) op = b, b = c, c = op;

		printf("%c %c %c\n", a, b, c);

	}

	return 0;

}
------------------------------------------------------
Problem1024:
#include <stdio.h>

int main()

{

	int a = 0, b = 0, c = 0,settimes = 0, times = 0;

	scanf("%d", &settimes);

	for (times = 0; times < settimes; times++)

	{

		scanf("%d", &b);

		for (a = 1; a <= b; a++)

		{

			c = c + a;

		}

		printf("%d\n", c);

		c = 0;

	}

	return 0;

}
