Problem1652:
#include<stdio.h>

int main()

{

	int i, a, b, c;

	for (i = 100; i <= 999; i++)

	{

		a = (i % 10)*(i % 10)*(i % 10);

		b = ((i / 10) % 10)*((i / 10) % 10)*((i / 10) % 10);

		c = (i / 100)*(i / 100)*(i / 100);



		if (i == a + b + c)

			printf("%d\n", i);

	}

	return 0;

}
