Problem1676:
#include <stdio.h>

int main()

{

	int t;

	scanf("%d", &t);

	while (t--)

	{

		int n, ans = 0;

		scanf("%d", &n);

		for (int i = 1; i <= 81; i++)

		{

			if (n - i < 0)

			{

				break;

			}

			int tmp = n - i, sum = 0;

			while (tmp != 0)

			{

				sum += tmp % 10;

				tmp /= 10;

			}

			if (sum == i)

			{

				ans++;

			}

		}

		printf("%d\n", ans);

	}

}
------------------------------------------------------
Problem1677:
#include <stdio.h>

#include <math.h>



int main()

{

	int t;

	scanf("%d", &t);

	while (t--)

	{

		int n;

		scanf("%d", &n);

		long long ans = 1;

		for (int i = 2; i <= sqrt(n); i++)

		{

			int cnt = 0;

			while (n % i == 0)

			{

				n /= i;

				cnt++;

			}

			if (i % 4 == 1)

			{

				ans *= 2 * cnt + 1;

			}

		}

		if (n > 1 && n % 4 == 1)

		{

			ans *= 3;

		}

		printf("%lld\n", ans * 4);

	}

	return 0;

}
