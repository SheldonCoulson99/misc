Problem1178:
#include<stdio.h>

int main()

{

	int n, i, j, k, a[101], b[101], search, t;

	scanf("%d", &n);

	for (i = 0; i < n; i++)

	{

		scanf("%d", &a[i]);

	}

	scanf("%d", &search);

	for (i = 0; i < n - 1; i++)

	{

		for (j = 0; j < n - 1 - i; j++)

		{

			if (a[j] > a[j + 1])

			{

				t = a[j];

				a[j] = a[j + 1];

				a[j + 1] = t;

			}

		}

	}

	for (i = 0; i < n; i++)

	{

		b[i] = a[n - i - 1];

	}

	for (i = 0; i < n; i++)

	{

		if (search == b[i])

		{

			printf("%d\n", i + 1);

			break;

		}

	}

	return 0;

}
