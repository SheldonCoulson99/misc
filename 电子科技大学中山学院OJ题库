Problem1019:
#include<iostream>

using namespace std;

int main()

{

	int n, a, b, c;

	cin >> n;

	a = n / 100;

	b = n % 100 / 10;

	c = n % 100 % 10;

	if (a*a*a + b*b*b + c*c*c == n) cout << "yes" << endl;

	else cout << "no" << endl;

}
