Problem1021:
#include <stdio.h>

int main()

{

	int day,month,year,sum,jump;

	//printf("请输入年月日：（用空格隔开）\n");

	scanf("%d %d %d",&year,&month,&day);

		switch(month)

			{

			case 1:

				sum=0;

				break;

			case 2:

				sum=31;

				break;

			case 3:

				sum=59;

				break;

			case 4:

				sum=90;

				break;

			case 5:

				sum=120;

				break;

			case 6:

				sum=151;

				break;

			case 7:

				sum=181;

				break;

			case 8:

				sum=212;

				break;

			case 9:

				sum=243;

				break;

			case 10:

				sum=273;

				break;

			case 11:

				sum=304;

				break;

			case 12:

				sum=334;

				break;

			default:

				printf("data error");

				break;

		 }



	sum=sum+day;



	if(year%400==0||(year%4==0&&year%100!=0))

			jump=1;

	else

		    jump=0;

	if(jump==1&&month>2)

		    sum++;

		    printf("%d\n",sum);

	

		return 0;

  }
