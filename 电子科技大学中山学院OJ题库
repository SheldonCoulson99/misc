Problem1168:
#include<stdio.h>

int main()

{

	int i, a, b, res[10];

	int count = 0, flag = 0;



	for (i = 1; i <= 7; i++)

	{

		scanf("%d%d", &a, &b);

		res[i] = a + b;

	}

	for ( i = 1; i <= 7; i++)

	{

		if (res[i]>8)

		{

			flag = i;

			break;

		}

		else

		{

			count++;

		}



	}

	if (count==7)

	{

		printf("0\n");

	}

	else

	{

		printf("%d\n", flag);

	}

	return 0;

}
