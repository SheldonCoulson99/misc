Problem1675:
#include<stdio.h>

int main()

{

	int T;

	scanf("%d", &T);

	while (T--)

	{

		long long n, m;

		scanf("%lld%lld", &n, &m);

		printf("%lld\n", n*m - n - m);

	}

	return 0;

}
