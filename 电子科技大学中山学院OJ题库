Problem1032:
#include<stdio.h>

int main()

{

	int i, a, b, c, n, m, sum = 0;

	scanf("%d%d", &n, &m);

		for (i = 100; i <= m; i++)

		{

			a = (i % 10)*(i % 10)*(i % 10);

			b = ((i / 10) % 10)*((i / 10) % 10)*((i / 10) % 10);

			c = (i / 100)*(i / 100)*(i / 100);

			

			if (i == a + b + c)

			{

				sum++;

				

			}

			

		}

		printf("%d\n", sum);

		return 0;

	}
------------------------------------------------------
Problem1033:
#include <stdio.h>

int main()

{

	int i, j, a, b;

	int count;

	count = 0;

	do 

		scanf("%d%d", &a, &b);

	while (a>b);

	for (i = a; i <= b; i++)

	{

		for (j = 2; j<i; j++)

			if (i%j == 0) break;

		if (i == j)

			count++;// = i;

	}

	printf("%d", count);

}
------------------------------------------------------
Problem1033:
#include <stdio.h>

int main()

{

	int i, j, a, b;

	int count = 0;

	do 

		scanf("%d%d", &a, &b);

	while (a>b);

	for (i = a; i <= b; i++)

	{

		for (j = 2; j<i; j++)

			if (i%j == 0) 

				break;

		if (i == j)

			count++;

	}

	printf("%d", count);

	return 0;

}
