#define max 100000

int a[max];

int main()

{

	int n, cnt;

	memset(a, 0, sizeof(int)*max);

	while (cin >> n)

	{

		cnt = 0;

		for (int i = 1; i <= n; i++)

		{

			if (n%i == 0)

				cnt++;

		}

		if (cnt % 2 == 0)

			cout << 0 << endl;

		else 

			cout << 1 << endl;

	}

	return 0;

}


