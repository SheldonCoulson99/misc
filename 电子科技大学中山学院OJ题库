Problem1539:
#include<iostream>

#include<cstring>

#include<cstdio>

#define MAXM 200010



using namespace std;



bool vis[MAXM];

int main() 

{

	int T, n, m;

	scanf("%d", &T);

	while (T--) 

	{

		scanf("%d%d", &n, &m);

		int s = 0;

		memset(vis, 0, sizeof(vis));

		vis[0] = 1;

		bool flag = 0;

		for (int i = 0; i<n; i++) 

		{

			int v;

			scanf("%d", &v);

			s = (s + v) % m;

			if (vis[s] == 1)

				flag = 1;

			vis[s] = 1;

		}

		if (flag) 

			puts("Stay here");

		else 

			puts("I will go with you");

	}

	return 0;

}
