Problem1646:
#include<stdio.h>

main()

{

	int i, n, a[1000], b, c = 0;

	scanf("%d", &n);

	for (i = 0; i<n; i++)

		scanf("%d", &a[i]);

	scanf("%d", &b);

	for (i = 0; i<n; i++)

	{

		if (b == a[i])

		{

			i++;

			printf("%d\n", i);

			break;

		}

		c++;

		if (c == n)

			printf("%d\n", -1);

	}

	return 0;

}
