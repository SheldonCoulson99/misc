Problem1039:
#include <stdio.h>

#include <math.h>

int main()

{

	int n, m;

	while (~scanf("%d %d", &n, &m))

	{

		double ans = n, f = n;

		for (int i = 1; i<m; i++)

		{

			f = sqrt(f);

			ans = ans + f;

		}

		printf("%.2lf\n", ans);

	}

	return 0;

}
------------------------------------------------------
Problem1039:
#include<stdio.h>

#include<math.h>



main()

{

	int x = 0, y = 0;

	double res = 0, fres = 0, x2 = 0;

	while (~scanf("%d%d", &x,&y))

	{

		double x2 = x, res = x;

		for (int j = 1; j < y; j++)

		{

			x2 = sqrt(x2);

			res += x2;

		}

		printf("%.2lf\n", res);

	}

}
