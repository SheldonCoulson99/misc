	int m = sizeof(a) / sizeof(a[0]);



	scanf("%d", &t);

	for (n = 0; n < t; n++)

	{

		for ( k = 0; k < 3; k++)

		{

			scanf("%d", &a[k]);

		}

		for (i = 0; i < m - 1; i++)

		{

			for (j = 0; j < m - i - 1; j++)

			{

				if (a[j] > a[j + 1])

				{

					tmp = a[j + 1];

					a[j + 1] = a[j];

