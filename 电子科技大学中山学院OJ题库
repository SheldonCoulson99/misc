Problem1018:
#include<stdio.h>

int main()

{

	int t;

	scanf("%d",&t);

	t = t / 10;



	switch (t)

	{

	case 10:

		printf("A");

		break;

	case 9:

		printf("A");

		break;

	case 8:

		printf("B");

		break;

	case 7:

		printf("C");

		break;

	case 6:

		printf("D");

		break;



	default:

		printf("E");

		break;

		return 0;

	}

}

				
