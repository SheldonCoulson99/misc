#include<stdio.h>

int main()

{

	int n, t = 0, T;

	char c;

	scanf("%d ", &T);

	while (T--) 

	{

		scanf("%c", &c);

		{

			scanf("%d", &n);

			/*if (t != 0)

				printf("\n");*/

			int i, j;

			for (i = 1; i <= n; i++)

			{

				for (j = 1; j <= n - i; j++)

					printf(" ");

				for (j = 1; j <= 2 * i - 1; j++)

				{

					if (i == 1 || i == n)

						printf("%c", c);

					else

					{

						if (j == 1 || j == 2 * i - 1)

							printf("%c", c);

						else

							printf(" ");

					}

				}

				printf("\n");

			}

			t = 1;

			getchar();

			printf("\n");

		}

	}

	return 0;

}
