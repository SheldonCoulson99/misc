Problem1076:
#include<stdio.h>



long cp(int n) 

{

	if (n == 1) 

		return 0;

	if (n == 2) 

		return 1;

	else return (cp(n - 1) + cp(n - 2))*(n - 1);



}

long p(int n, int m) 

{

	long s1 = 1, s2 = 1, a = n, b = m;

	for (int i = 1; i <= m; i++) 

	{

		s1 = s1*(a--);

	}

	for (int i = 1; i <= m; i++) 

	{

		s2 = s2*(b--);

	}

	return s1 / s2;

}



int main() 

{

	int c, n, m;

	scanf("%d", &c);

	while (c--) 

	{

		scanf("%d%d", &n, &m);

		printf("%ld\n", cp(m)*p(n, m));

	}

	return 0;

}


