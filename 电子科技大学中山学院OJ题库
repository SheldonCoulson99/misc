Problem1104:
#include<stdio.h>

#include<math.h>



int main()

{

	int r, l, w;

	int s;



	scanf("%d%d%d", &r, &l, &w);

	if (r >= 1 && r <= 1000 && l >= 1 && l <= 1000 && w >= 1 && w <= 1000)

	{

		s = sqrt((double)(l*l) + (double)(w*w));

		if (s > (2 * r))

		{

			printf("no\n");

		}

		else

		{

			printf("yes\n");

		}

	}

	else

		return 0;

}
