Problem1131:
#include<stdio.h>

int main()

{

	long i, j, k, n, m;

	int t,c=0;

	scanf("%d", &t);

	for (m = 0; m < t; m++)

	{

		scanf("%ld", &n);

		if (n <= 10000)

		{

			k = 10;

			c = 0;

			for (i = 1; i <= n; i++)

			{

				if (i == k)

					k *= 10;

				j = i*i;

				if (j%k == i)

					c++;

			}

			printf("%d\n", c);

		}

		else

			;

	}

	return 0;

}
