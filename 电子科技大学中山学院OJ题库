Problem1175:
#include<stdio.h>

int main()

{

	int m, i, a, b, c, x, y, z;



	scanf("%d", &m);

	a = 0; b = 0; c = 0;



	for (i = 0; i<m; i++)

	{

		scanf("%d%d%d", &x, &y, &z);

		if (x > a || x == a && y < b || x == a && y == b && z > c)

		{

			a = x;

			b = y; 

			c = z;

		}

	}

	printf("%d\n", c);

	return 0;

}


