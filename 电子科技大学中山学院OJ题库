Problem1082:
#include<stdio.h>

int main()

{

	int n,i;

	int a,sum=0;

	char ch;

	scanf("%d",&n);

	for ( i = 0; i < n; i++)

	{

		scanf(" %c %d",&ch,&a);

		if (ch>=65 && ch<=90)

		{

			ch-=64;

			sum = ch + a;

		}

		if (ch>=97 && ch<=122)

		{

			ch-=96;

			ch = -ch;

			sum = ch + a;



		}

		printf("%d\n",sum);

	}

	



}
