Problem1528:
#include<stdio.h>

int main()

{

	int T, res[10000] = {0};

	int a, b, max;

	int i, j, k;



	scanf("%d", &T);

	for ( i = 0; i < T;  i++)

	{

		int P;

		scanf("%d", &P);

		if (P <= 10000 && P > 0)

		{

			for (j = 0; j < P; j++)

			{

				scanf("%d%d", &a, &b);

				res[j] = abs(a - b);

			}

		}

		max = res[0];

		for ( k = 0; k < P; k++)

		{

			if (max < res[k])

			{

				max = res[k];

			}

		}

		printf("%d\n", max);

	}

	return 0;

}
