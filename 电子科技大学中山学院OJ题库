Problem1287:
#include<stdio.h>

int main()

{

	int n, a[30];

	int xi = 0;

	int t, i, j;

	int res = 0;



	scanf("%d", &n);

	if (n < 1 && n>20)

	{

		return 0;

	}

	else

	{

		for (i = 0; i < n; i++)

		{

			scanf("%d", &a[i]);

		}

		for (i = 0; i < n; i++)

		{

			for (j = 0; j < n - i - 1; j++)

			{

				if (a[j] > a[j + 1])

				{

					t = a[j + 1];

					a[j + 1] = a[j];

					a[j] = t;

				}

			}

		}

		for (i = 0; i < n; i++)

		{

			res = (a[n-1] - a[0]) * 2;

			

		}

		printf("%d\n", res);

		return 0;

	}

}
