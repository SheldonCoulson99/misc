Problem1086:
#include<stdio.h>

int main()

{

	char ch, s[300];

	int top = 0;

	while (scanf("%c", &ch) != EOF)

	{

		if (ch == '(' || ch == '[' || ch == '{')

			s[++top] = ch;

		if (ch == ')'&&s[top] == '(' || ch == ']'&&s[top] == '[' || ch == '}'&&s[top] == '{')

			top--;

		else if (ch == ')' || ch == ']' || ch == '}') 

			s[++top] = ch;

		if (ch=='#'||ch=='@')

		{

			if (top == 0)

				printf("Wonderful\n");

			else

				printf("Be Careful\n");

			top = 0;

		}

		if (ch == '@')

			return 0;

	}

}
