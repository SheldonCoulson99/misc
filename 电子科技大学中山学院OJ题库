#include<stdio.h>

int main(int argc, char* argv[])

{

	int ans=1;

	int m,n,i;



	scanf("%d",&m);

	scanf("%d",&n);



	for(i=1;i<=n;i++)

	{

		ans=(ans*(m-n+i))/i;

	}



	printf("%d\n",ans);

return 0;

