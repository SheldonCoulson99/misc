Problem1078:
#include<stdio.h>

int main()

{

	int transfer(int x);

	int x;

	while (scanf("%d", &x) != EOF&& x > 0 && x < 1000)

	printf("%d\n", transfer(x));

}

int transfer(int x)

{

	int p = 1, y = 0, s;

	while (1)

	{

		s = x % 2;

		x /= 2;

		y += s*p;

		p *= 10;

		if (x<2)

		{

			y += x*p;

			break;

		}

	}

	return y;

}
