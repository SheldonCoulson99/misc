Problem1281:
#include<stdio.h>

#include<string.h>

int main()

{

	int n, m, k, b;

	char str[101];

	int i, j;

	scanf("%d", &n);

    for ( i=0;i<n;i++ ) 

	{   

		b=1; scanf("%s",str);

		for ( j=0,k=str[0],m=0;j<strlen(str);j++ ) 

			if ( str[j]==k ) 

			{ 

				m++; 

				if ( m>6 ) 

				{ 

					b=0;

					break; 

				}    

			} 

			else

			{ 

				m=1;

				k=str[j];

			} 

		if (b) 

			printf("NO\n"); 

		else 

			printf("YES\n");

	}

	return 0;

}
