Problem1046:
#include<stdio.h>

int num(int n);

int main() 

{

	int k, n;

	while (scanf("%d", &n) && n != 0)

		printf("%d\n", num(n));

	return 0;

}

int num(int n)

{

	int i;

	int a1[55] = { 0 }, a2[55] = { 0 }, a3[55] = { 0 }, a4[55] = { 0 };

	//a1 第n年活1年的个数

	//a2 第n年活2年的个数

	//a3 第n年活3年的个数 

	//a4 第n年活4年及以上的个数 

	a4[1] = 1;

	for (i = 2; i <= n; i++)

	{

		a1[i] = a3[i - 1] + a4[i - 1];

		a2[i] = a1[i - 1];

		a3[i] = a2[i - 1];

		a4[i] = a3[i - 1] + a4[i - 1];

	}

	return a1[n] + a2[n] + a3[n] + a4[n];

}


