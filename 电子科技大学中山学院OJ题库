Problem1635:
#include<stdio.h>

int main()



{

	int n, m[200];

	int i, j, t;

	scanf("%d", &n);

	for (i = 0; i < n; i++)

	{

		scanf("%d", &m[i]);

	}



	for (i = 0; i<n - 1; i++)

	{

		for (j = 0; j<n - i - 1; j++)

		{

			if (m[j]>m[j + 1])

			{

				t = m[j + 1];

				m[j + 1] = m[j];

				m[j] = t;

			}

		}

	}



	for ( i = 0; i < n; i++)

	{

		printf("%d ", m[i]);

	}

	printf("\n");

	return 0;

}


