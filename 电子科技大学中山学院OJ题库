Problem1081:
#include<iostream>

#include<string>

using namespace std;

bool equal(string a, string b)

{

	string::iterator aItr = a.begin(), bItr = b.begin();

	bool point = false;

	bool aPL = true, bPL = true;

	bool Zero = true;

	if (*a.begin() == '-')

	{

		aPL = false;

		++aItr; 

	}

	else if (*a.begin() == '+')

		++aItr;



	if (*b.begin() == '-')

	{

		bPL = false;

		++bItr;

	}

	else if (*b.begin() == '+')

		++bItr;

	while (aItr != a.end() && *aItr == '0')

		++aItr;

	while (bItr != b.end() && *bItr == '0')

		++bItr;



	while (aItr != a.end() && bItr != b.end() && *aItr == *bItr)

	{

		if ((*aItr != '0' && *aItr != '.') || (*bItr != '0' && *bItr != '.'))

			Zero = false;

		if (*aItr == '.')

			point = true;

		++aItr;

		++bItr;

	}

	if (aItr != a.end() && *aItr == '.')

	{

		point = true;

		++aItr;

	}

	if (bItr != b.end() && *bItr == '.')

	{

		point = true;

		++bItr;

	}

	if (point)

	{

		while (aItr != a.end() && *aItr == '0')

			++aItr;

		while (bItr != b.end() && *bItr == '0')

			++bItr;

	}

	if ((aItr != a.end() && *aItr != '0') || (bItr != b.end() && *bItr != '0'))

	{

		Zero = false;

	}

	if (aItr == a.end() && bItr == b.end() && aPL == bPL)

		return true;

	if (Zero)

		return true;

	return false;

}



int main()

{

	string a, b;

	while (cin >> a >> b)

	{

		if (equal(a, b))

			cout << "YES" << endl;

		else

			cout << "NO" << endl;

	}

	return 0;

}

