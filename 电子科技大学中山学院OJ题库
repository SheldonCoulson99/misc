Problem1674:
#include<stdio.h>

#define N 1000

int main()

{

	int T, n;

	int a[N] = {0};



	scanf("%d ", &T);

	for (int i = 0; i < T; i++)

	{

		scanf("%d ", &n);

		if (n >= 1 && n <= 1000)

		{

			for (int j = 0; j < n; j++)

			{

				scanf("%d", &a[j]);

			}

			for (int l = 0; l < n; l++)

			{

				if (a[l] % 2 != 0)

					a[l]++;

			}

			for (int x = 0; x < n; x++)

			{

				printf("%d", a[x]);

				printf(" ");

			}

			printf("\n");

		}

		else

		{

			return 0;

		}

	}

}
