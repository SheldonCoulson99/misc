Problem1538:
#include<cstdio>

#include<algorithm>

#include<iostream>



using namespace std;



double a[5];

bool ok;

void dfs(int step, double ans) 

{

	if (ok) return;

	if (step>2) 

	{

		if (ans == a[3]) ok = true;

		return;

	}

	for (int i = 0; i<4; i++)

	{

		if 

			(i == 0) dfs(step + 1, ans + a[step]);

		else if 

			(i == 1) dfs(step + 1, ans - a[step]);

		else if 

			(i == 2) dfs(step + 1, ans*a[step]);

		else if 

			(i == 3) dfs(step + 1, ans / a[step]);

	}

}

int main()

{

	int T;

	scanf("%d", &T);

	while (T--) 

	{

		for (int i = 0; i<4; i++) 

			scanf("%lf", &a[i]);

		ok = false;

		sort(a, a + 4);

		do {

			dfs(1, a[0]);

			if (ok) break;

		} while (next_permutation(a, a + 4));

		printf("%s\n", ok ? "Yes" : "No");

	}

	return 0;

}
