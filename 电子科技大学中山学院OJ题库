#include<stdio.h>



int main()

{

	int n;

	int a[500];



	scanf("%d", &n);

	for (int i = 1; i <= n; ++i)

	{

		scanf("%d", &a[i]);

	}

	for (int i = 1; i <= n - 1; ++i)

	{

		for (int j = 1 + i; j <= n; ++j)

		{

			if (a[i] > a[j])

			{

				int t;

				t = a[i];

				a[i] = a[j];

				a[j] = t;

			}

		}

	}

	for (int i = 1; i <= n / 2; ++i)

	{

		printf("%d ", a[i * 2 - 1]);

	}

	printf("%d ", a[n]);

	for (int i = n / 2; i >= 1; --i)

	{

		printf("%d ", a[i * 2]);

	}

	return 0;

}
