Problem1283:
#include<stdio.h>

#include<string.h>

int main()

{

	int T;

	char s[50000];

	scanf("%d ", &T);

	while (T--)

	{

		gets(s);

		{

			for (int i = 0; i< strlen(s); i++)

			{

				if (s[i] == '1')

					s[i] = 'I';

				else if (s[i] == '0')

					s[i] = 'O';

				else if (s[i] == '3')

					s[i] = 'E';

				else if (s[i] == '4')

					s[i] = 'A';

				else if (s[i] == '7')

					s[i] = 'T';

				else if (s[i] == '5')

					s[i] = 'S';

			}

			for (int i = 0; i < strlen(s); i++)

				printf("%c", s[i]);

			printf("\n");

		}

	}

	return 0;

}
