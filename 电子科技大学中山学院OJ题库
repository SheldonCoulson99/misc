Problem1670:
#include<stdio.h>

int main()

{

	int n;

	int i, j, k, l, m, o;

	int h;

	scanf("%d", &h);

	for (int u = 0; u < h; u++)

	{

		scanf("%d", &n);

		for (i = 1; i <= (n + 1) / 2; i++)

		{

			for (j = 1; j <= (n + 1) / 2 - i; j++)

				printf(" ");

			for (k = 1; k <= 2 * i - 1; k++)

				printf("*");

			printf("\n");

		}

		for (l = 1; l <= n - (n + 1) / 2; l++)

		{

			for (m = l; m >= 1; m--)

				printf(" ");

			for (o = n - 2 * l; o >= 1; o--)

				printf("*");

			printf("\n");

		}

	}

	return 0;

}
