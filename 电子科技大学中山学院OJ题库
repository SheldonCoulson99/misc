Problem1169:
#include <stdio.h>  

int main()

{

	int a[10];

	int i;

	int b=0;

	int count=0;



	

	for (i = 0; i<10; i++)

	{

		scanf("%d", &a[i]);

	}

	scanf("%d", &b);

	b = b + 30;



	for (i = 0; i<10; i++)

	{

		if (b >= a[i]) count++;

	}



	printf("%d\n", count);

	return 0;

}
