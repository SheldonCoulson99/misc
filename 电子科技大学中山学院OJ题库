Problem1672:
#include<stdio.h>

#include<string.h>

int main()

{

	int T,l;

	char a[105] = {0};

	int c = 0;

	scanf("%d ", &T);

	for (int i = 0; i < T; i++)

	{

		c = 0;

		gets(a);

		l = strlen(a);

		for (int j = 0; j < l; j++)

		{

			if (a[j] == '0' || a[j] == '4' || a[j] == '6' || a[j] == '9')

			{

				c++;

			}

			if (a[j]=='8')

			{

				c += 2;

			}

		}

		printf("%d\n", c);

	}

	return 0;

}
