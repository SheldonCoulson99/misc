Problem1165:
#include<iostream>

#include<cstdio>

using namespace std;

int main()

{

	int n;

	int a[500];



	cin >> n;

	for (int i = 1; i <= n; ++i)

	{

		cin >> a[i];

	}

	for (int i = 1; i <= n - 1; ++i)

	{

		for (int j = 1 + i; j <= n; ++j)

		{

			if (a[i] > a[j])

			{

				int t;

				t = a[i];

				a[i] = a[j];

				a[j] = t;

			}

		}

	}

	for (int i = 1; i <= n / 2; ++i)

		cout << a[i * 2 - 1] << " ";

	cout << a[n] << " ";

	for (int i = n / 2; i >= 1; --i)

		cout << a[2 * i] << " ";

	return 0;

}
