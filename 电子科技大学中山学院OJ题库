#include <stdio.h>

int main()

{

	int i, c, n, res;

	double a[21], b[21];

	a[0] = 1;

	for (i = 1; i < 21; i++)

		a[i] = a[i - 1] * i; 

	b[1] = 0;

	b[2] = 1;

	b[3] = 2;

	for (i = 3; i < 21; i++)

		b[i] = (i - 1) * (b[i - 1] + b[i - 2]);



	while (scanf("%d", &c) != EOF)

		for (i = 0; i < c; i++)

		{

			scanf("%d", &n);

			//res = b[n] * 100 / a[n];

			printf("%.2lf%%\n", b[n] * 100 / a[n]);

		}

	return 0;

}
