Problem1300:
#define _CRT_SECURE_NO_WARNINGS

#include<stdio.h>

#include<string.h>

int main()

{

	int n, ca = 0, cb = 0;

	int i, j;

	char c[1000000];



	scanf("%d ", &n);

	for (i = 0; i < n; i++)

	{

		gets(c);

		for (j = 0; c[j] != '\0'; j++)

		{

			if (c[j] >= 'a' && c[j] <= 'm')

				ca++;

			else if (c[j] >= 'n' && c[j] <= 'z')

				cb++;

		}



		if (ca > cb)

			printf("1\n");

		else if (ca < cb)

			printf("2\n");

		else if (ca == cb)

			printf("0\n");

		ca = 0, cb = 0;

	}

	return 0;

}
