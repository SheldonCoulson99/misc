Problem1051:
#include<stdio.h>

#include<string.h>



double student[55], subject[9];

double score[55][9];



int main()

{

	int n, m;

	while (scanf("%d%d", &n, &m) != EOF)

	{

		for (int i = 0; i<n; i++)

			for (int j = 0; j<m; j++)

				scanf("%lf", &score[i][j]);

		double sumstu, sumsub;

		for (int i = 0; i<n; i++)

		{

			sumstu = sumsub = 0;

			for (int j = 0; j<m; j++)

			{

				sumstu += score[i][j];

			}

			student[i] = sumstu / m;

		}

		for (int i = 0; i<m; i++)

		{

			sumsub = 0;

			for (int j = 0; j<n; j++)

				sumsub += score[j][i];

			subject[i] = sumsub / n;

		}

		int sum = 0, flag;

		for (int i = 0; i<n; i++)

		{

			flag = 0;

			for (int j = 0; j<m; j++)

			{

				if (score[i][j]<subject[j])

				{

					flag = 1;

					break;

				}

			}

			if (flag == 0)

				sum++;

		}

		for (int i = 0; i<n - 1; i++)

			printf("%.2lf ", student[i]);

		printf("%.2lf\n", student[n - 1]);

		for (int i = 0; i<m - 1; i++)

			printf("%.2lf ", subject[i]);

		printf("%.2lf\n", subject[m - 1]);

		printf("%d\n\n", sum);

	}

	return 0;

}
