Problem1671:
#include<stdio.h>

int main()

{

	int T, n, m, M[100], c = 0;

	scanf("%d", &T);

	for (int i = 0; i < T; i++)

	{

		c = 0;

		scanf("%d%d", &n, &m);

		if (n >= 1 && n <= 100 && m <= 100 && m >= 1)

		{

			for (int j = 0; j < m; j++)

			{

				scanf("%d", &M[j]);

			}

			/*for (int k = 0; k < m; k++)

			{

				for (int l = 0; l < m - k - 1; l++)

				{

					if (M[l] > M[l + 1])

					{

						int t;

						t = M[l];

						M[l] = M[l + 1];

						M[l + 1] = t;

					}

				}

			}*/

			for (int o = 0; o < m; o++)

			{

				if (n >= M[o])

				{

					c++;

					n -= M[o];

				}

				if (n <= 0)

				{

					break;

				}

			}

			printf("%d\n", c);

		}

		else

		{

			return 0;

		}

	}

}
