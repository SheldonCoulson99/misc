Problem1084:
#include<cstdio>

#include<cstring>

#include<iostream>

#include<utility>

#include<string>

#include<vector>

#include<algorithm>

#include<queue>

#include<cstdlib>

#include<cmath>

#include<stack>



using namespace std;



char a[1000000];



int main()

{

	int n;

	cin >> n;

	while (n--)

	{

		cin >> a;

		int l = strlen(a), k, sum = 0;

		for (k = 0; k < l; ++k)

			sum += a[k] - '0';

		while (sum > 9)

		{

			int temp = sum;

			sum = 0;

			while (temp>0)

			{

				sum += temp % 10;

				temp /= 10;

			}

		}

		printf("%d\n", sum);

	}



	return 0;

}


