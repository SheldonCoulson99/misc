Problem1130:
#include<stdio.h>

#include<math.h>

int main()

{

	long int n, m;

	double Pi = 0.0, pn = 0.0;

	int p = 1, cp = 4;

	scanf("%d", &n);

	while (n--)

	{

		scanf("%d", &m);

		

		for (int i = 0; i < m; i++)

		{

			

			pn = cp / (double)p;

			Pi += pn;

			p += 2;

			cp = -cp;

		}

		printf("%.5lf\n", fabs(Pi));

		p = 1;

		Pi = 0.0; 

	}

	return 0;

}
