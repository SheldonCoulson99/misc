Problem1153:
#include<stdio.h>

#include<string.h>

#include<ctype.h>



int main()

{

	char s[256];

	int sum, i;

	while (gets(s) && s[0] != '#')

	{

		sum = 0;

		for ( i = 0; i < strlen(s); i++)

		{

			if (s[i] != ' ')

				sum += (((int)s[i] - 64) *(i+1));

		}

		printf("%d\n", sum);

	}

	return 0;

}
