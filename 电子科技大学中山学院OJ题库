Problem1216:
#include<stdio.h>

#include<string.h>



int main()

{

	char s[50];

	int l;

	int sum = 0;

	int i, j=1;



	while (scanf("%s", s)!=EOF)

	{

		int sum = 0;

		int i, j = 1;

		l = strlen(s);

		

		for (i = l - 1; i >= 0; i--)

		{

			sum += (s[i] - '0')*j;

			j *= 8;

		}

		printf("%d\n", sum);

	}

	return 0;

}
