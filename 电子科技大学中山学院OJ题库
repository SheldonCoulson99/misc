#include <cmath> 

#include <cstring> 



using namespace std;

const int a = 100;

char str[a + 1];

char t[a + 1];



int main()

{

	int j = 0, n, i, len, f;

	cin >> n;

	cin.get();

	while (n--)

	{

		char t[a + 1] = { 0 };

		f = 1;

		j = 0;

		cin.getline(str, a + 1);

		for (i = 0; str[i] != '\0'; i++)

		{

			if (

				(str[i] >= 'a'&&str[i] <= 'z')

				|| (str[i] >= 'A'&&str[i] <= 'Z')

				)

			{

				t[j] = str[i];

				j++;

			}

		}

		/*for (i=0;t[i]!='\0';i++)

		{

		cout << t[i] << endl;

		}

		*/

		len = strlen(t);

		for (i = 0; i<len / 2; i++)

		{

			if ((t[i] != t[len - 1 - i]) && abs(t[i] - t[len - 1 - i]) != 32)

			{

				f = 0;

			}

		}

		if (f == 0)

		{

			cout << "No" << endl;

		}

		else

		{

			cout << "Yes" << endl;

		}



	}

	return 0;

}

