			{

				c++;

			}

			if (hct < 36 || hct>40)

			{

				c++;

			}

			if (plt<100||plt>300)

			{

				c++;

			}

			if (c==0)

			{

				printf("normal\n");

			}

			else

			{

				printf("%d\n", c);

			}

		}

		else

		{

			return 0;

		}

	}

		return 0;

}
