Problem1085:
#include <iostream>

#include <algorithm>

#include <iomanip>



using namespace std;

double avg(double a[], int n) {

	double sum = 0;

	for (int i = 0; i<n; i++) {

		sum += a[i] * (n - i);

	}

	return sum / n;

}



int main()

{

	double time[1000];

	int m, n;

	cin >> m;

	while (m--)

	{

		while (cin >> n)

		{

			for (int i = 0; i < n; i++)

			{

				cin >> time[i];

			}

			sort(time, time + n);

			//for (int i = 0; i < n; i++)

			//	cout << time[i] << " ";

			//cout << endl;

			double ans = avg(time, n);

			cout<< setiosflags(ios::fixed) <<setprecision(2)<< double(ans)<<endl;

		}

	}

	return 0;

}
