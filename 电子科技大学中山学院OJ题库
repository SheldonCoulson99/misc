Problem1050:
#include <math.h>

#include <stdio.h>



int main()

{

	int i, j;

	int n, m;

	int x, y;

	double a, t;



	while (scanf("%d%d", &n, &m) != EOF)

	{

		a = x = y = 0;

		for (i = 1; i <= n; i++)

		{

			for (j = 1; j <= m; j++)

			{

				scanf("%lf", &t);

				if (fabs(t) > fabs(a))

				{

					a = t;

					x = i;

					y = j;

				}

			}

		}

		printf("%d %d %.0f\n", x, y, a);

	}



	return 0;

}
