Problem1159:
#include<stdio.h>

#include<math.h>

int main()

{

	double r, l,sqrt2=0.0;

	sqrt2 = sqrt(2);

	double sr, sl;

	while (scanf("%lf%lf", &r, &l) != EOF)

	{

		

		sl = l * sqrt2;

		if ( r > (sl/2.0) )

		{

			printf("R\n");

		}

		else if ( r <= (l/2.0) )

		{

			printf("L\n");

		}

		else if (r > (l/2.0)&&r<(sl/2.0))

		{

			printf("NO\n");

		}

	}

	return 0;

}
