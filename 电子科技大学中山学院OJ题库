Problem1059:
#include<stdio.h>

#include<math.h>



int main()

{

	int n,m;

	int i;

	int t, b;

	char a[100];

	char s[] = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";

	while (scanf("%d%d", &n,&m) != EOF)

	{

		t = n;

		t = (int)fabs(t);

		i = 0;

		while (t>0)

		{

			b = t%m;

			if (b >= 10)

				a[i] = s[b - 10];

			else

				a[i] = b + '0';

			t /= m;

			i++;

		}

		i--;

		if (n < 0)

			printf("-");

		while (i>=0)

		{

			printf("%c", a[i]);

			i--;

		}

		printf("\n");

	}

	return 0;

}
