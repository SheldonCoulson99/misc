Problem1266:
#include <stdio.h>

#define N 50



int main()

{

	int a[N] = {0};

	int i, j, t, k, l;

	int n, m;



	scanf("%d", &m);

	for (n = 0; n < m; n++)

	{

		scanf("%d", &l);

		for (k = 0; k <= l; k++)

		{

			scanf("%d", &a[k]);

		}

		for (i = 0; i < l; i++)

		{

			for (j = 0; j < l - i; j++)

			{

				if (a[j] > a[j + 1])

				{

					t = a[j + 1];

					a[j + 1] = a[j];

					a[j] = t;

				}

			}

		}

		for (i = 0; i <= l; i++)

		{

			printf("%d ", a[i]);

		}

		printf("\n");

	}

	return 0;

}
