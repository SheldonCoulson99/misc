Problem1647:
#include<stdio.h>

int max(int a, int b, int c)  

{

	if (a>b&&a>c)

		return a;

	else if (b>a&&b>c)

		return b;

	else

		return c;

}

int main()

{

	int x, y, z, i, j, k;

	scanf("%d%d%d", &x, &y, &z); 

	k = max(x, y, z);          

	i = 1;

	while(1)

	{

		j = k * i;

		if ((j%x == 0) && (j%y == 0) && (j%z == 0))

			break;

		i++;

	}

	printf("%d\n", j);

	return 0;

}
