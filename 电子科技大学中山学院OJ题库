Problem1133:
#include<stdio.h>

#include<math.h>

#define T 10000001

int a[T];

int main()

{

	int i, j, n, m, sum;

	for (i = 2; i <= sqrt(T); ++i)

	{

		if (a[i] != 1)

		{

			for (j = i + i; j <= T; j += i)

			{

				a[j] = 1;

			}

		}

	}

	for (i = 2, sum = 0; i <= T; ++i)

	{

		if (a[i] != 1)

		{

			sum++;

		}

		a[i] = sum;

	}

	scanf("%d", &n);

	while (n--)

	{

		scanf("%d", &m);

		printf("%d\n", a[m]);

	}

	return 0;

}
