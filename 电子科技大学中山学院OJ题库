Problem1164:
#include<stdio.h>

#include<string.h>

#define N 200



int main()

{

	int l,i;

	char a[N];

	scanf("%s", a);

	l = strlen(a);

	int flag = 0;

	while (l % 2 == 0)

	{

		for (i = 0; i <= l / 2; i++)

		{

			if (a[i] != a[l - i - 1])

			{

				flag = 1;

				break;

			}

		}



		if (flag == 0)

			l /= 2;

		else

		{

			printf("%d\n", l);

			break;

		}

	}

	if (l % 2 != 0)

	{

		printf("%d\n", l);

	}

	return 0;

}
