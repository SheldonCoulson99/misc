Problem1219:
#include<stdio.h>

#include<string.h>

int main()

{

	int n, s[26], i, j, max;

	char a[1000];



	scanf("%d", &n);

	for (j = 0; j < n; j++)

	{

		scanf("%s", a);

		for (i = 0; i < 26; i++)

			s[i] = 0;

		for (i = 0; i < strlen(a); i++)

			s[a[i] - 'a']++;

		max = 0;

		for (i = 1; i < 26; i++)

			if (s[i] > s[max])

				max = i;

		printf("%c %d\n", max + 'a', s[max]);

	}

}
