Problem1040:
#include<stdio.h>

main()

{

	int i, n;

	float s, h, m, j;

	int a[100];

	scanf("%d", &n);

	for (i = 1; i <= n; i++)

	{

		scanf("%d", &a[i]);

	}

	for (i = 1; i <= n; i++)

	{

		s = 0; h = -1;

		for (j = 1; j <= a[i]; j++)

		{

			h = -h;

			m = h*(1 / j);

			s = s + m;

		}

		printf("%.2f\n", s);

	}

}
------------------------------------------------------
Problem1041:
#include<stdio.h>

#include<math.h>

main()

{

	int x, y, n;

	int i, b;

	int res;

	double k;



	while (scanf("%d%d", &x, &y) && (x || y))

	{

		b = 0;

		for (n = x; n <= y; n++)

		{

			res = n*n + n + 41;

			k = sqrt(res);

			for (i = 2; i <= k; i++)

				if (res %i == 0)

					break;

			if (i > k)

				b++;

		}

		if (y - x + 1 == b)

			printf("OK\n");

		else

			printf("Sorry\n");

	}

}
------------------------------------------------------
Problem1041:
#include<stdio.h>

#include<math.h>

main()

{

	int x, y, n;

	int i, b;

	int res;

	double k;



	while (scanf("%d%d", &x, &y) && (x == y == 0))

	{

		b = 0;

		for (n = x; n <= y; n++)

		{

			res = n*n + n + 41;

			k = sqrt(res);

			for (i = 2; i <= k; i++)

				if (res %i == 0)

					break;

			if (i > k)

				b++;

		}

		if (y - x + 1 == b)

			printf("OK\n");

		else

			printf("Sorry\n");

	}

}
------------------------------------------------------
Problem1042:
#include<stdio.h>

main()

{

	int n = 0, i = 0; 

	double finalscore = 0, tempscore = 0, score = 0, max = 0, min = 0;

	while (scanf("%d", &n)!= EOF)

	{

		scanf("%lf", &tempscore);

		score = max = min = tempscore;

		for (i = 1; i < n; i++)

		{

			scanf("%lf", &tempscore);

			score += tempscore;

			max = (tempscore > max) ? tempscore : max;

			min = (tempscore < min) ? tempscore : min;

		}

		finalscore = (score - min - max)/(n-2);

		printf("%.2lf\n", finalscore);

	}

	return 0;

}
